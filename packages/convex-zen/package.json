{
  "name": "convex-zen",
  "version": "1.0.1",
  "description": "Production-grade auth component for Convex",
  "repository": {
    "type": "git",
    "url": "https://github.com/natedunn/convex-zen"
  },
  "type": "module",
  "bin": {
    "convex-zen": "./dist/cli/index.js"
  },
  "main": "./src/client/index.ts",
  "types": "./src/client/index.ts",
  "exports": {
    ".": {
      "types": "./src/client/index.ts",
      "import": "./src/client/index.ts"
    },
    "./tanstack-start": {
      "types": "./src/client/tanstack-start.ts",
      "import": "./src/client/tanstack-start.ts"
    },
    "./tanstack-start/identity-jwt": {
      "types": "./src/client/tanstack-start-identity-jwt.ts",
      "import": "./src/client/tanstack-start-identity-jwt.ts"
    },
    "./tanstack-start/plugins": {
      "types": "./src/client/tanstack-start-plugins.ts",
      "import": "./src/client/tanstack-start-plugins.ts"
    },
    "./tanstack-start-client": {
      "types": "./src/client/tanstack-start-client.ts",
      "import": "./src/client/tanstack-start-client.ts"
    },
    "./tanstack-start-client/plugins": {
      "types": "./src/client/tanstack-start-client-plugins.ts",
      "import": "./src/client/tanstack-start-client-plugins.ts"
    },
    "./react": {
      "types": "./src/client/react.ts",
      "import": "./src/client/react.ts"
    },
    "./plugins/admin": {
      "types": "./src/client/plugins/admin.ts",
      "import": "./src/client/plugins/admin.ts"
    },
    "./convex.config": {
      "types": "./src/component/convex.config.ts",
      "import": "./src/component/convex.config.ts"
    }
  },
  "publishConfig": {
    "main": "./dist/client/index.js",
    "types": "./dist/client/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/client/index.d.ts",
        "import": "./dist/client/index.js"
      },
      "./tanstack-start": {
        "types": "./dist/client/tanstack-start.d.ts",
        "import": "./dist/client/tanstack-start.js"
      },
      "./tanstack-start/identity-jwt": {
        "types": "./dist/client/tanstack-start-identity-jwt.d.ts",
        "import": "./dist/client/tanstack-start-identity-jwt.js"
      },
      "./tanstack-start/plugins": {
        "types": "./dist/client/tanstack-start-plugins.d.ts",
        "import": "./dist/client/tanstack-start-plugins.js"
      },
      "./tanstack-start-client": {
        "types": "./dist/client/tanstack-start-client.d.ts",
        "import": "./dist/client/tanstack-start-client.js"
      },
      "./tanstack-start-client/plugins": {
        "types": "./dist/client/tanstack-start-client-plugins.d.ts",
        "import": "./dist/client/tanstack-start-client-plugins.js"
      },
      "./react": {
        "types": "./dist/client/react.d.ts",
        "import": "./dist/client/react.js"
      },
      "./plugins/admin": {
        "types": "./dist/client/plugins/admin.d.ts",
        "import": "./dist/client/plugins/admin.js"
      },
      "./convex.config": {
        "types": "./dist/component/convex.config.d.ts",
        "import": "./dist/component/convex.config.js"
      }
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@noble/curves": "^1.9.7",
    "@noble/hashes": "^1.8.0",
    "convex": "^1.17.0",
    "hash-wasm": "^4.11.0",
    "jose": "^5.10.0"
  },
  "devDependencies": {
    "@tanstack/react-start": "^1.162.1",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.8",
    "@vitest/coverage-v8": "^2.0.0",
    "convex-test": "^0.0.33",
    "react": "^19.0.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "@tanstack/react-start": "^1.162.1",
    "convex": "^1.17.0",
    "react": ">=18.0.0 || >=19.0.0"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-start": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "license": "Apache-2.0"
}
